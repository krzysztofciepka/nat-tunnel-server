{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "max-classes-per-file": "off",
        "class-methods-use-this": "off",
        "new-cap": "off",
        "no-underscore-dangle": "off",
        "import/prefer-default-export": "off",
        "consistent-return": "off"
    }
}